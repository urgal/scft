<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:include schemaLocation="basetypes.xsd"/>
	<xs:include schemaLocation="objecttypes.xsd"/>
	<xs:complexType name="responseType198">
		<xs:annotation>
			<xs:documentation>Ответ на запрос начала работы точки. Открытия смены (пачки)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="responseHeader">
				<xs:sequence>
					<xs:element name="body">
						<xs:annotation>
							<xs:documentation>Тело ответа</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="code" type="stringType" use="required">
								<xs:annotation>
									<xs:documentation>Код ответа хоста</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="publicKey" type="stringType">
								<xs:annotation>
									<xs:documentation>Открытй ключ для шифрования ПИН-блока</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="response" type="responseType198"/>
</xs:schema>
